{
  "name": "@thib3113/boks-sdk",
  "version": "0.0.9",
  "description": "Core SDK for Boks Parcel Box communication and PIN generation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thib3113/boks-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/thib3113/boks-sdk-js/issues"
  },
  "homepage": "https://thib3113.github.io/boks-sdk-js/",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LEGALS.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": "v25.7.0"
  },
  "packageManager": "pnpm@10.0.0",
  "main": "dist/cjs/boks-sdk.cjs",
  "module": "dist/esm/boks-sdk.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/boks-sdk.js",
      "require": "./dist/cjs/boks-sdk.cjs"
    },
    "./core": {
      "types": "./dist/types/core.d.ts",
      "import": "./dist/esm/core.js",
      "require": "./dist/cjs/core.cjs"
    },
    "./simulator": {
      "types": "./dist/types/simulator.d.ts",
      "import": "./dist/esm/simulator.js",
      "require": "./dist/cjs/simulator.cjs"
    },
    "./browser": "./dist/boks-sdk.js",
    "./browser-core": "./dist/boks-core.js"
  },
  "unpkg": "dist/boks-sdk.js",
  "jsdelivr": "dist/boks-sdk.js",
  "scripts": {
    "build": "node build.js",
    "docs": "pnpm run docs:build && mkdir -p public/examples && cp -r examples/* public/examples/ && typedoc",
    "test": "vitest run",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "docs:dev": "vitepress dev wiki",
    "docs:build": "vitepress build wiki"
  },
  "keywords": [
    "boks",
    "smart-lock",
    "parcel-box",
    "ble",
    "sdk"
  ],
  "author": "Thib3113",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/node": "22.19.11",
    "@types/web-bluetooth": "0.0.21",
    "esbuild": "0.25.12",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "fast-check": "4.5.3",
    "globals": "17.3.0",
    "prettier": "3.8.1",
    "typedoc": "0.28.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitepress": "1.6.4",
    "vitest": "3.2.4",
    "vue": "3.5.28"
  }
}
